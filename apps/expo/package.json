{
  "name": "expo-app",
  "version": "1.1.1",
  "main": "index.js",
  "scripts": {
    "android": "TAMAGUI_TARGET=native npx expo run:android",
    "build:android:output": "eas build --platform android --profile development --local --output /home/runner/work/PackRat/PackRat/app-release.apk --json --non-interactive",
    "build:android:preview": "eas build --platform android --profile preview --local",
    "build:android:production": "eas build --platform android --profile production --local",
    "build:android": "yarn run prebuild:clean && yarn run build:android:preview",
    "build:clean": "yarn run prebuild:clean && yarn run build",
    "build:ios:output": "eas build --platform ios --profile development --local --output /home/runner/work/PackRat/PackRat/app-release.ipa --json --non-interactive",
    "build:ios": "eas build --platform ios --profile production --local",
    "build:ios:preview": "eas build --platform ios --profile preview --local",
    "copy:app": "cp app.example.json app.json",
    "copy:env": "test -f .env && echo '.env already exists, skipping copy' || (echo 'Copying .env.example to .env' && cp .env.example .env)",
    "copy:gradle": "cp gradle.properties.example gradle.properties",
    "copy:pod": "cp Podfile.example Podfile",
    "ios": "TAMAGUI_TARGET=native npx expo run:ios",
    "lint": "eslint . --fix",
    "move:gradle": "mv gradle.properties android/gradle.properties",
    "move:pod": "mv Podfile ios/Podfile",
    "prebuild:clean": "npx expo prebuild --clean",
    "prebuild": "npx expo prebuild",
    "setup:copy": "yarn copy:env && yarn copy:app && yarn copy:gradle && yarn copy:pod",
    "setup:keyreplace": "node setup.js",
    "setup:move": "yarn move:gradle && yarn move:pod",
    "setup": "yarn setup:copy && yarn setup:keyreplace && yarn setup:move",
    "start-native": "TAMAGUI_TARGET=native npx expo start -c -d",
    "start-web": "TAMAGUI_TARGET=web npx expo start -c",
    "start": "EXPO_USE_METRO_WORKSPACE_ROOT=1 npx expo start --dev-client -c -d",
    "test": "echo \"No tests yet\"",
    "web": "npx expo start --web"
  },
